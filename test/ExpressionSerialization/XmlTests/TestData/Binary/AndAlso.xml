<?xml version="1.0" encoding="utf-8"?>
<!-- (a, b) => (a AndAlso b) -->
<expression
    xmlns="urn:schemas-vm-com:Linq.Expressions.Serialization"
    xmlns:i="http://www.w3.org/2001/XMLSchema-instance">
    <lambda
        type="boolean">
        <parameters>
            <parameterDefinition
                type="boolean"
                name="a"
                id="P1" />
            <parameterDefinition
                type="boolean"
                name="b"
                id="P2" />
        </parameters>
        <body>
            <andAlso
                type="boolean">
                <parameterReference
                    type="boolean"
                    name="a"
                    idref="P1" />
                <parameterReference
                    type="boolean"
                    name="b"
                    idref="P2" />
            </andAlso>
        </body>
    </lambda>
</expression>