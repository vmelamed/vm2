<?xml version="1.0" encoding="utf-8"?>
<!-- (a, b) => (a AndAlso b) -->
<expression
    xmlns="urn:schemas-vm-com:Linq.Expressions.Serialization.Xml"
    xmlns:i="http://www.w3.org/2001/XMLSchema-instance">
    <lambda
        type="boolean">
        <parameters>
            <parameter
                type="boolean"
                id="P1"
                name="a" />
            <parameter
                type="boolean"
                id="P2"
                name="b" />
        </parameters>
        <body>
            <andAlso
                type="boolean">
                <parameter
                    type="boolean"
                    id="P1"
                    name="a" />
                <parameter
                    type="boolean"
                    id="P2"
                    name="b" />
            </andAlso>
        </body>
    </lambda>
</expression>