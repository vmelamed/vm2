<?xml version="1.0" encoding="utf-8"?>
<!-- (a, b) => (a * b) -->
<expression
    xmlns="urn:schemas-vm-com:Linq.Expressions.Serialization"
    xmlns:i="http://www.w3.org/2001/XMLSchema-instance">
    <lambda
        type="int">
        <parameters>
            <parameter
                type="int"
                name="a" />
            <parameter
                type="int"
                name="b" />
        </parameters>
        <body>
            <multiplyChecked
                type="int">
                <parameter
                    type="int"
                    name="a" />
                <parameter
                    type="int"
                    name="b" />
            </multiplyChecked>
        </body>
    </lambda>
</expression>