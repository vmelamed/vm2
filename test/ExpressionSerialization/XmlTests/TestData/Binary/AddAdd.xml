<?xml version="1.0" encoding="utf-8"?>
<!-- (a, b, c) => ((a + b) + c) -->
<expression
    xmlns="urn:schemas-vm-com:Linq.Expressions.Serialization"
    xmlns:i="http://www.w3.org/2001/XMLSchema-instance">
    <lambda>
        <parameters>
            <parameter
                type="int"
                name="a" />
            <parameter
                type="int"
                name="b" />
            <parameter
                type="int"
                name="c" />
        </parameters>
        <body>
            <add>
                <add>
                    <parameter
                        type="int"
                        name="a" />
                    <parameter
                        type="int"
                        name="b" />
                </add>
                <parameter
                    type="int"
                    name="c" />
            </add>
        </body>
    </lambda>
</expression>