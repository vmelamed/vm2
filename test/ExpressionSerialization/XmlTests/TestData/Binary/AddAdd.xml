<?xml version="1.0" encoding="utf-8"?>
<!-- (a, b, c) => ((a + b) + c) -->
<expression
    xmlns="urn:schemas-vm-com:Linq.Expressions.Serialization"
    xmlns:i="http://www.w3.org/2001/XMLSchema-instance">
    <lambda
        type="int">
        <parameters>
            <parameterDefinition
                type="int"
                name="a"
                id="P1" />
            <parameterDefinition
                type="int"
                name="b"
                id="P2" />
            <parameterDefinition
                type="int"
                name="c"
                id="P3" />
        </parameters>
        <body>
            <add
                type="int">
                <add
                    type="int">
                    <parameter
                        type="int"
                        name="a"
                        idref="P1" />
                    <parameter
                        type="int"
                        name="b"
                        idref="P2" />
                </add>
                <parameter
                    type="int"
                    name="c"
                    idref="P3" />
            </add>
        </body>
    </lambda>
</expression>