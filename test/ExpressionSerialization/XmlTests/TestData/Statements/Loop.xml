<?xml version="1.0" encoding="utf-8"?>
<!-- {var _result; ... } -->
<expression
    xmlns="urn:schemas-vm-com:Linq.Expressions.Serialization"
    xmlns:i="http://www.w3.org/2001/XMLSchema-instance">
    <block>
        <variables>
            <variableDefinition
                type="int"
                name="_result"
                id="P1" />
        </variables>
        <assign
            type="int">
            <variableDefinition
                type="int"
                name="_value"
                id="P2" />
            <constant>
                <!-- 5 -->
                <int>5</int>
            </constant>
        </assign>
        <assign
            type="int">
            <variable
                type="int"
                name="_result"
                idref="P1" />
            <constant>
                <!-- 1 -->
                <int>1</int>
            </constant>
        </assign>
        <loop
            type="void">
            <block>
                <conditional
                    type="void">
                    <greaterThan
                        type="boolean">
                        <variable
                            type="int"
                            name="_value"
                            idref="P2" />
                        <constant>
                            <!-- 1 -->
                            <int>1</int>
                        </constant>
                    </greaterThan>
                    <block>
                        <multiplyAssign
                            type="int">
                            <variable
                                type="int"
                                name="_result"
                                idref="P1" />
                            <postDecrementAssign
                                type="int">
                                <variable
                                    type="int"
                                    name="_value"
                                    idref="P2" />
                            </postDecrementAssign>
                        </multiplyAssign>
                        <goto
                            type="void"
                            kind="continue">
                            <target
                                name="continue"
                                idref="L2" />
                        </goto>
                    </block>
                    <goto
                        type="void"
                        kind="break">
                        <target
                            name="break"
                            idref="L1" />
                    </goto>
                </conditional>
            </block>
            <continueLabel>
                <target
                    name="continue"
                    id="L2" />
            </continueLabel>
            <breakLabel>
                <target
                    name="break"
                    id="L1" />
            </breakLabel>
        </loop>
    </block>
</expression>