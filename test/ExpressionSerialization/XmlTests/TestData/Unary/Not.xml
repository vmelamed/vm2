<?xml version="1.0" encoding="utf-8"?>
<!-- a => Not(a) -->
<expression
    xmlns="urn:schemas-vm-com:Linq.Expressions.Serialization"
    xmlns:i="http://www.w3.org/2001/XMLSchema-instance">
    <lambda>
        <parameters>
            <parameter
                type="boolean"
                name="a" />
        </parameters>
        <body>
            <not>
                <parameter
                    type="boolean"
                    name="a" />
            </not>
        </body>
    </lambda>
</expression>