<?xml version="1.0" encoding="utf-8" ?>
<!-- () => new [] {"aaa", "bbb", "ccc"} -->
<expression xmlns="urn:schemas-vm-com:Linq.Expressions.Serialization"
    xmlns:i="http://www.w3.org/2001/XMLSchema-instance">
    <lambda>
        <parameters />
        <body>
            <newArrayInit type="string">
                <elements>
                    <constant>
                        <string>aaa</string>
                    </constant>
                    <constant>
                        <string>bbb</string>
                    </constant>
                    <constant>
                        <string>ccc</string>
                    </constant>
                </elements>
            </newArrayInit>
        </body>
    </lambda>
</expression>